body
	background -webkit-linear-gradient(left, #C852FF 0%, #42E8C7 100%)
@font-face{
	font-family HennyPenny;
	src url(HennyPenny-Regular.otf)
	font-family quicksand-dash;
	src url(Quicksand-LightItalic.otf) 
}
canvas
	background-color rgba(30,30,30,0.2)
	margin 0 14%
	border-right 2.3px dotted #232323
	border-left 2.3px dotted #232323
section
	margin 0 6% 0 8%
	button
		background rgba(30,30,30,0.2)
		width 49.77%
		border 2px solid #232323
		height 6em
		font-size 1.5em
		font-family quicksand-dash
	button:hover
		background rgba(245, 245, 245, .30)
p
	text-align center
	font-size 3em 
	font-family quicksand-dash
a
	text-decoration none
	color #232323
footer
	margin-top  2em
	text-align center
	height 8em
	background-color rgba(245, 256, 256, .8)
	font-family HennyPenny
	p
		font-size 3em
		margin 0
	&:after
		content "-"
	&:before
		content "-"
